{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Vasilis Chatzipanagiotis <vchatzipan@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "start": "npm run develop & npm run start:lambda",
    "start:lambda": "netlify-lambda serve ./src/lambda/netlify",
    "develop": "gatsby develop",
    "deploy-lambda": "./scripts/deploy-lambda.sh updateLocations && ./scripts/deploy-lambda.sh updateStaticData",
    "build": "gatsby build && yarn run build:lambda",
    "build:lambda": "netlify-lambda build ./src/lambda/netlify",
    "fetch-static-data": "node ./src/lambda/aws/updateStaticData.js",
    "fetch-locations": "node ./src/lambda/aws/updateLocations.js",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@turf/helpers": "^6.1.4",
    "@turf/inside": "^5.0.0",
    "@turf/turf": "^5.1.6",
    "babel-eslint": "^10.0.1",
    "cheap-ruler": "^2.5.1",
    "classnames": "^2.2.6",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "gatsby": "^2.0.53",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.16",
    "gatsby-plugin-react-css-modules": "^2.0.4",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sharp": "^2.0.14",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.1.8",
    "http-proxy-middleware": "^0.19.1",
    "lodash.set": "^4.3.2",
    "mapbox-gl": "^0.52.0",
    "netlify-lambda": "^1.1.1",
    "node-fetch": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "unfetch": "^4.0.1"
  },
  "devDependencies": {
    "gatsby-plugin-postcss": "^2.0.2",
    "gatsby-plugin-react-svg": "^2.0.0",
    "node-sass": "^4.11.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.15.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chatzipan/oasa.live"
  },
  "bugs": {
    "url": "https://github.com/chatzipan/oasa.live/issues"
  }
}
